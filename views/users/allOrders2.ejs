                            <th>Order Date</th>
                            <th>Status</th>
                            <th>Total Price</th>
                            <th>Action</th>
                            


                            <% dates.forEach(function(user){ %>
                         </tr>
                         <!-- start row-->
            
                         <td>

                            <div class="accordion">
                                <div class="accordion-section">
                                  <a class="accordion-section-title" href="#accordion-1"><%=user.date%></a>
                                  <div id="accordion-1" class="accordion-section-content">
                                    <p> <%user.component%></p>
                                  </div><!--end .accordion-section-content-->
                                </div><!--end .accordion-section-->

                         </td>
                           
                                <td><%=user.status%></td> 
                                <td><%=user.totalprice%></td>    

                                <td>
                                        <%if (user.status=="inprocessing"){ %>
                                        cancel
                                        <%}%>
                                    
                                    
                                    
                                    </td>


                                    
                                     
                                    </tr
                                    <%})%>>                  
                          <!-- end row-->

                         

                       </table>
                       
                    
                    
                    </form>
                   </div>
                <!-- end order now-->
                
